<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.MoreFragment">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessParent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
    <androidx.cardview.widget.CardView
    android:layout_width="match_parent"
    android:elevation="10dp"
    android:foreground="?android:selectableItemBackground"
    android:clickable="true"
    android:focusable="true"
    android:layout_margin="10dp"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/dashboard"
        android:layout_width="match_parent"
        android:text="Dashboard"
        android:drawableStart="@drawable/ic_dashboard2"
        android:padding="15dp"
        android:drawableEnd="@drawable/ic_chevron_right_black_24dp"
        android:drawablePadding="10dp"
        android:textSize="16sp"
        android:textColor="@android:color/secondary_text_light"
        android:layout_height="wrap_content"/>
</androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:elevation="10dp"
        android:foreground="?android:selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:layout_margin="10dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/profile"
            android:layout_width="match_parent"
            android:text="Profile"
            android:drawableStart="@drawable/ic_user"
            android:padding="15dp"
            android:drawableEnd="@drawable/ic_chevron_right_black_24dp"
            android:drawablePadding="10dp"
            android:textSize="16sp"
            android:textColor="@android:color/secondary_text_light"
            android:layout_height="wrap_content"/>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:elevation="10dp"
        android:foreground="?android:selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:layout_margin="10dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/payment_history"
            android:layout_width="match_parent"
            android:text="Payment History"
            android:drawableStart="@drawable/ic_history"
            android:padding="15dp"
            android:drawableEnd="@drawable/ic_chevron_right_black_24dp"
            android:drawablePadding="10dp"
            android:textSize="16sp"
            android:textColor="@android:color/secondary_text_light"
            android:layout_height="wrap_content"/>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:elevation="10dp"
        android:foreground="?android:selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:layout_margin="10dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/payment_method"
            android:layout_width="match_parent"
            android:text="Payment Method"
            android:drawableStart="@drawable/ic_credit_card"
            android:padding="15dp"
            android:drawablePadding="10dp"
            android:textSize="16sp"
            android:drawableEnd="@drawable/ic_chevron_right_black_24dp"
            android:textColor="@android:color/secondary_text_light"
            android:layout_height="wrap_content"/>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:elevation="10dp"
        android:foreground="?android:selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:layout_margin="10dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/reviews"
            android:layout_width="match_parent"
            android:text="Reviews"
            android:drawableEnd="@drawable/ic_chevron_right_black_24dp"
            android:drawableStart="@drawable/ic_rating"
            android:padding="15dp"
            android:drawablePadding="10dp"
            android:textSize="16sp"
            android:textColor="@android:color/secondary_text_light"
            android:layout_height="wrap_content"/>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:elevation="10dp"
        android:foreground="?android:selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:layout_margin="10dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/notification"
            android:layout_width="match_parent"
            android:text="Notification"
            android:drawableEnd="@drawable/ic_chevron_right_black_24dp"
            android:drawableStart="@drawable/ic_bell"
            android:padding="15dp"
            android:drawablePadding="10dp"
            android:textSize="16sp"
            android:textColor="@android:color/secondary_text_light"
            android:layout_height="wrap_content"/>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:elevation="10dp"
        android:foreground="?android:selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:layout_margin="10dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/help"
            android:layout_width="match_parent"
            android:text="Support Center"
            android:drawableStart="@drawable/ic_question"
            android:padding="15dp"
            android:drawablePadding="10dp"
            android:textSize="16sp"
            android:drawableEnd="@drawable/ic_chevron_right_black_24dp"
            android:textColor="@android:color/secondary_text_light"
            android:layout_height="wrap_content"/>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:elevation="10dp"
        android:foreground="?android:selectableItemBackground"
        android:clickable="true"
        android:focusable="true"
        android:layout_margin="10dp"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/logout"
            android:layout_width="match_parent"
            android:text="LogOut"
            android:drawableStart="@drawable/ic_logout"
            android:padding="15dp"
            android:drawableEnd="@drawable/ic_chevron_right_black_24dp"
            android:drawablePadding="10dp"
            android:textSize="16sp"
            android:textColor="@android:color/secondary_text_light"
            android:layout_height="wrap_content"/>
    </androidx.cardview.widget.CardView>
            <View
                android:layout_width="match_parent"
                android:layout_height="50dp" />
    </LinearLayout>
    </ScrollView>
</LinearLayout>
